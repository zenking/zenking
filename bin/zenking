#!/usr/local/bin/node
var argv = require('argv');
var echo = require('../lib/index');
var args = argv.option({
        name: 'URL',
        short: 'u',
        type: 'string',
        description: '确定需要设置的URL',
        example: "'script --URL=value' or 'script -u value'"

}).run();

console.log(echo(args.options));
