#!/usr/local/bin/node
var argv = require('argv');
var echo = require('../lib/index');
var args = argv.option({
        name: 'option',
        short: 'o',
        type: 'string',
        description: 'Defines an option for your script',
        example: "'script --opiton=value' or 'script -o value'"

}).run();

console.log(echo(args.options));
